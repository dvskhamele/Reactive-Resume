{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "cd /vercel/workpath && npm install && cd apps/client && npm install && npx vite build",
        "outputDirectory": "apps/client/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/client/dist/index.html"
    }
  ]
}